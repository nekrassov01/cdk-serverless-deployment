{
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^context$" }
    ]
  },
  "env": {
    "es2021": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
